void colorSetup() {
  if ( yellowC==true ) stroke(yellow);
  if ( pinkC==true ) stroke(pink);
  if ( greenC==true ) stroke(green);
  if ( blueC==true ) stroke(blue);
  if ( purpleC==true ) stroke(purple);
  if ( redC==true ) stroke(red);
  if ( blackC==true ) stroke(black);
  if ( orangeC==true ) stroke(orange);
  if ( eraseC==true ) stroke(resetBackground);
}
